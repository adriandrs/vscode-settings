// LANGUAGE FORMATTING PREFERENCES
{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // WINDOW APPEARANCE AND BEHAVIOR
  "apc.electron": {
    "backgroundColor": "#00000000",
    "frame": false,
    "vibrancy": "fullscreen-ui",
    "visualEffectState": "active",
    "titleBarStyle": "hidden",
    "trafficLightPosition": {
      "x": 15,
      "y": 14
    }
  },
  "window.commandCenter": false,
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "window.newWindowDimensions": "inherit",
  "window.zoomLevel": 0.05,
  "window.autoDetectColorScheme": true,
  "window.menuBarVisibility": "compact",

  // FILE SETTINGS
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 10000,
  "files.exclude": {
    "/.classpath": true,
    "/.factorypath": true,
    "/.project": true,
    "/.settings": true
  },
  "files.watcherExclude": {
    "/.git/objects/": true,
    "/node_modules/": true
  },

  // EDITOR APPEARANCE AND BEHAVIOR
  "editor.lineNumbers": "on",
  "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
  "editor.lineHeight": 2,
  "editor.fontLigatures": true,
  "editor.fontSize": 11.6,
  "editor.fontWeight": "380",
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lightbulb.enabled": "off",
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.minimap.autohide": true,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.accessibilitySupport": "off",
  "editor.renderControlCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "auto",
  "editor.suggestSelection": "first",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.showFoldingControls": "mouseover",
  "editor.glyphMargin": false,
  "editor.padding.top": 0,
  "editor.inlayHints.padding": true,
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontFamily": "Cascadia Code",
  "editor.folding": true,
  "editor.foldingStrategy": "auto",
  "editor.renderLineHighlight": "none",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.matchBrackets": "never",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.smoothScrolling": true,
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,

  // TERMINAL CONFIGURATION
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font, Consolas, 'Courier New', monospace",
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 11.5,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontWeight": "380",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-powershell",
      "path": [
        "${env:windir}\\Sysnative\\powershell",
        "${env:windir}\\System32\\powershell.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.hideOnStartup": "always",
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.env.windows": {},

  // GITLENS CONFIGURATION
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.blame.line.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.graph.minimap.enabled": false,
  "gitlens.graph.minimap.additionalTypes": ["tags"],
  "gitlens.graph.layout": "editor",
  "gitlens.views.repositories.showIncomingActivity": true,
  "gitlens.views.workspaces.showIncomingActivity": true,

  // FILE EXPLORER SETTINGS
  "symbols.hidesExplorerArrows": false,
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 1,
  "explorer.decorations.colors": false,
  "explorer.decorations.badges": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.autoReveal": true,

  // INDENTATION SETTINGS
  "indenticator.showIndentGuide": false,
  "indenticator.color.dark": "rgba(255,255,255,0.1)",
  "indenticator.color.light": "rgba(0,0,0,0.1)",
  "indenticator.hover.peekBack": 100,
  "indenticator.hover.peekForward": 100,
  "indenticator.inner.hover.peekBack": 0,
  "indenticator.inner.hover.peekForward": 1,
  "indenticator.showCurrentDepthInStatusBar": false,
  "indenticator.width": 0.1,

  // GIT SETTINGS
  "git.decorations.enabled": true,
  "git.autofetch": true,
  "git.mergeEditor": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.confirmSync": false,

  // WORKBENCH CONFIGURATION
  "workbench.panel.defaultLocation": "bottom",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.startupEditor": "none",
  "workbench.layoutControl.type": "both",
  "workbench.editor.enablePreview": true,
  "workbench.preferredHighContrastLightColorTheme": "Atom One Light",
  "workbench.reduceMotion": "on",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "symbols",
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "workbench.layoutControl.enabled": false,
  "workbench.editor.labelFormat": "short",
  "workbench.tips.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.preferredLightColorTheme": "Tokyo Night Light",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.tree.enableStickyScroll": false,

  // BREADCRUMBS SETTINGS
  "breadcrumbs.enabled": false,
  "breadcrumbs.icons": false,

  // DIFF AND SCM SETTINGS
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "scm.providers.visible": 0,
  "scm.showIncomingChanges": "true",
  "scm.showOutgoingChanges": "true",
  "scm.providerCountBadge": "auto",
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "scm.diffDecorations": "gutter",

  // GITHUB COPILOT SETTINGS
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "java": false,
    "yaml": false,
    "javascript": true,
    "typescriptreact": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,

  // TAILWIND SETTINGS
  "tailwind-fold.unfoldIfLineSelected": false,
  "tailwind-fold.autoFold": false,

  // CHAT AND MARKDOWN SETTINGS
  "chat.editor.fontSize": 11,
  "chat.editor.wordWrap": "on",
  "chat.setupFromDialog": true,
  "markdown-preview-enhanced.codeBlockTheme": "auto.css",
  "markdown-preview-enhanced.previewTheme": "none.css",

  // TYPESCRIPT SETTINGS
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.inlayHints.parameterTypes.enabled": false,

  // AUTOFOLD IMPORTS SETTINGS
  "autofoldImports.onStartOrOpen": false,
  "autofoldImports.onSave": false,
  "autofoldImports.onEditorChange": false,

  // APC CUSTOMIZATION
  "apc.font.family": "Cascadia Code, Consolas, 'Courier New', monospace",
  "apc.header": {
    "height": 32,
    "fontSize": 11
  },
  "apc.sidebar.titlebar": {
    "height": -20,
    "fontSize": -10
  },
  "apc.stylesheet": {
    ".tabs-container": "margin-bottom: 10px;"
  },
  "apc.listRow": {
    "height": 19.5,
    "fontSize": 11.5
  },
  "apc.menubar.compact": true,
  "apc.imports": [],

  // MISC EXTENSION SETTINGS
  "telemetry.telemetryLevel": "off",
  "update.mode": "default",
  "oneDarkPro.bold": true,
  "oneDarkPro.vivid": false,
  "supermaven.otherWarning": false,
  "supermaven.enable": {
    "*": false
  },
  "codemosModern.dark.auxiliaryUiTheme": null,
  "codemosModern.dark.design": "modern",
  "codemosModern.dark.accentColor": "#1c1e23",
  "codemosModern.dark.adaptiveMode": "moderate",
  "codemosModern.dark.auxiliaryCodeTheme": null,
  "vscode_vibrancy.opacity": 0.4,
  "cSpell.customDictionaries": {},
  "cSpell.language": "en,es-ES,la",
  "errorLens.fontWeight": "300",
  "console-ninja.featureSet": "Community",
  "background.style": {
    "background-position": "100% 100%",
    "background-size": "auto",
    "opacity": 0
  },
  "custom-ui-style.font.sansSerif": "Ubuntu Mono",

  // CUSTOM UI STYLES AND CSS IMPORTS
  "vscode_custom_css.imports": [
    "file:///C:/Users/adrs1/OneDrive/Documents/vscode-customization/vscode-script.js",
    "file:///C:/Users/adrs1/OneDrive/Documents/vscode-customization/custom-vscode.css"
  ],

  // SETTINGS CYCLE PRESETS
  "settings.cycle": [
    {
      "id": "breadcrumbs",
      "values": [
        {
          "breadcrumbs.enabled": false
        },
        {
          "breadcrumbs.enabled": true
        }
      ]
    },
    {
      "id": "lineNumbers",
      "values": [
        {
          "editor.lineNumbers": "off",
          "editor.folding": true
        },
        {
          "editor.lineNumbers": "on",
          "editor.folding": true
        }
      ]
    },
    {
      "id": "indentation",
      "values": [
        {
          "indenticator.showIndentGuide": false
        },
        {
          "indenticator.showIndentGuide": true
        }
      ]
    }
  ],

