// General editor settings
{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "apc.electron": {
    "backgroundColor": "#00000000",
    "frame": false,
    "vibrancy": "fullscreen-ui",
    "visualEffectState": "active",
    "titleBarStyle": "hidden",
    "trafficLightPosition": {
      "x": 15,
      "y": 14
    }
  },

  //Tittle bar settings
  "window.commandCenter": false,
  // "window.title": "VS Code",
  // "window.title": "${rootName}${separator}${activeEditorShort}",
  // "window.title": "${folderName} ${separator} ${activeEditorShort}",
  // "window.title": "${folder}",
  // "window.titleBarStyle": "custom",
  // "window.title": "${activeEditorShort}${separator}${rootName}",
  "window.title": "${rootName}",
  // "window.title": "${}",
  // "window.title": " ",
  // "window.titleSeparator": " | ",

  // Window and margin settings
  "window.newWindowDimensions": "inherit",
  "editor.lineNumbers": "off",
  "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
  // "editor.fontFamily": "Dank Mono, Consolas, 'Courier New', monospace",
  "editor.lineHeight": 1.8,
  // "editor.letterSpacing": 0.5,
  // "editor.tabSize": 2,
  // "editor.wordWrap": "on",
  // "editor.wordWrapColumn": 80,
  // "editor.wrappingIndent": "same",
  // "editor.wordWrapMinified": true,
  // "editor.cursorBlinking": "solid",
  // "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontLigatures": true,
  "editor.fontSize": 12.4,
  "editor.fontWeight": "370",
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lightbulb.enabled": "off",
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.occurrencesHighlight": "off",
  "editor.parameterHints.enabled": false,
  "editor.selectionHighlight": false,

  // GitHub Copilot settings
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "java": false,
    "yaml": false,
    "javascript": true,
    "typescriptreact": true
  },

  // Terminal settings
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.fontWeight": "380",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-powershell",
      "path": [
        "${env:windir}\\Sysnative\\powershell",
        "${env:windir}\\System32\\powershell.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },

  // GitLens settings
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.blame.line.enabled": false, // was working in previous versions
  "gitlens.currentLine.enabled": false, // in modern version
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,

  // File explorer settings
  "symbols.hidesExplorerArrows": false,
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 1,

  // File decorations settings
  "files.exclude": {
    "/.classpath": true,
    "/.factorypath": true,
    "/.project": true,
    "/.settings": true
  },
  "files.watcherExclude": {
    "/.git/objects/": true,
    "/node_modules/": true
  },

  // Minimap settings
  "editor.minimap.autohide": true,

  // Indentation guide settings
  "indenticator.showIndentGuide": true,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.indentation": false,
  "indenticator.color.dark": "rgba(255,255,255,0.1)",
  "indenticator.color.light": "rgba(0,0,0,0.1)",
  "indenticator.hover.peekBack": 100,
  "indenticator.hover.peekForward": 100,
  "indenticator.inner.hover.peekBack": 0,
  "indenticator.inner.hover.peekForward": 1,
  "indenticator.showCurrentDepthInStatusBar": false,
  "indenticator.width": 0.1,
  "editor.bracketPairColorization.enabled": false,
  // "editor.matchBrackets": "near",

  // Integrated terminal and Git settings
  "git.autofetch": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.startupEditor": "none",
  "workbench.layoutControl.type": "both",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.enablePreview": true,
  "window.zoomLevel": 0,
  "editor.multiCursorModifier": "alt",
  "window.density.editorTabHeight": "default",

